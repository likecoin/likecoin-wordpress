{
  "name": "likecoin-wordpress",
  "version": "4.0.0",
  "description": "",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev:admin:settings": "wp-scripts start likecoin/js/admin-settings/src/index --output-path=likecoin/assets/js/admin-settings",
    "dev:sidebar": "wp-scripts start likecoin/js/sidebar/src/index --output-path=likecoin/assets/js/sidebar",
    "build": "npm run build:blocks && npm run build:admin:settings && npm run build:sidebar",
    "build:blocks": "wp-scripts build --webpack-src-dir=likecoin/blocks --output-path=likecoin/assets/blocks",
    "build:admin:settings": "wp-scripts build likecoin/js/admin-settings/src/index --output-path=likecoin/assets/js/admin-settings",
    "build:sidebar": "wp-scripts build likecoin/js/sidebar/src/index --output-path=likecoin/assets/js/sidebar",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "wp-scripts lint-js",
    "lint:style": "wp-scripts lint-style --fix",
    "format:js": "wp-scripts format",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:pkg-json": "wp-scripts lint-pkg-json"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^6.1.3",
    "@wordpress/block-editor": "^12.26.0",
    "@wordpress/blocks": "^12.3.5",
    "@wordpress/data": "^9.2.8",
    "@wordpress/edit-post": "^5.0.17",
    "@wordpress/element": "^4.0.1",
    "@wordpress/i18n": "^4.2.2",
    "@wordpress/plugins": "^6.26.0",
    "@wordpress/wordcount": "^3.2.3",
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.4.4"
  },
  "devDependencies": {
    "@wordpress/scripts": "^30.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "stylelint-order": "^5.0.0"
  }
}
